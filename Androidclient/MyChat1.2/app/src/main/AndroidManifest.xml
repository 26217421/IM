<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lihanxiao.mychat">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/mychatlogo3"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/mychatlogo3"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <service
            android:name="com.example.lihanxiao.mychat.com.example.wangbw.mychat.SocketService"
            android:enabled="true"
            android:exported="true" >
        </service>

        <activity android:name="com.example.lihanxiao.mychat.com.example.wangbw.mychat.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.lihanxiao.mychat.com.example.wangbw.mychat.ChooseChatActivity"></activity>
    </application>

</manifest>